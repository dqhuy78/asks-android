<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="www.seotoolzz.com.Ask">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="www.seotoolzz.com.Ask.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="www.seotoolzz.com.Ask.Activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="www.seotoolzz.com.Ask.Activity.MainActivity" />
        </activity>
        <activity android:name="www.seotoolzz.com.Ask.Activity.SignUpActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="www.seotoolzz.com.Ask.Activity.MainActivity" />
        </activity>
        <activity android:name="www.seotoolzz.com.Ask.Activity.CreatNewQuestion">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="www.seotoolzz.com.Ask.Activity.MainActivity" />
        </activity>
        <activity android:name="www.seotoolzz.com.Ask.model.DetailQuestionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="www.seotoolzz.com.Ask.Activity.MainActivity" />
        </activity>
    </application>

</manifest>
